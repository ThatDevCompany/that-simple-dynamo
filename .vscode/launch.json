{
    "version": "0.2.0",
    "configurations": [
   
      {
        "type": "node",
        "request": "attach",
        "name": "Attach",
        "port": 9229
      },
      {
        "name": "Current TS File",
        "type": "node",
        "request": "launch",
        "args": ["${relativeFile}"],
        "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
        "sourceMaps": true,
        "cwd": "${workspaceRoot}",
        "protocol": "inspector"
      }
    ]
   }